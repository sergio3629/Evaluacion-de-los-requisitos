<!-- Actualizar Usuario -->
<h3>Actualizar Usuario</h3>
<table>
  <tr><td>Número</td> <td>3</td></tr>
  <tr><td>Nombre del Servicio</td> <td>Actualizar Usuario</td></tr>
  <tr><td>URI</td> <td>/actualizarUsuario/:identificacion</td></tr>
  <tr><td>Descripción</td> <td>Permite actualizar los datos de un usuario existente.</td></tr>
  <tr><td>Verbo HTTP</td> <td>PUT</td></tr>
  <tr><td>Header</td> <td>Authorization: Bearer &lt;token&gt;</td></tr>
  <tr><td>Request JSON</td> <td>
    <pre>{
  "nombre": "Nuevo nombre",
  "email": "nuevoemail@gmail.com",
  "password": "nueva contraseña"
}</pre>
  </td></tr>
  <tr><td>Respuesta JSON</td> <td>
    Correcto:<br>
    Status: 200<br>
    <pre>{
  "status": 200,
  "message": "Usuario actualizado correctamente"
}</pre>
    Incorrecto:<br>
    Status: 404<br>
    <pre>{
  "status": 404,
  "message": "No se encontró ningún usuario con esa identificación"
}</pre>
    Error:<br>
    Status: 500<br>
    <pre>{
  "status": 500,
  "message": "Error en el servidor: Descripción del error"
}</pre>
  </td></tr>
